{
    "name": "netgen/content-browser",
    "description": "Netgen Content Browser is a Symfony bundle that provides an interface which selects items from any kind of backend and returns the IDs of selected items back to the calling code.",
    "license": "MIT",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Netgen",
            "homepage": "https://www.netgenlabs.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-mbstring": "*",
        "netgen/content-browser-ui": "~1.0.0",
        "symfony/asset": "^3.4.24 || ^4.3",
        "symfony/config": "^3.4.24 || ^4.3",
        "symfony/console": "^3.4.24 || ^4.3",
        "symfony/debug": "^3.4.24 || ^4.3",
        "symfony/dependency-injection": "^3.4.24 || ^4.3",
        "symfony/event-dispatcher": "^3.4.24 || ^4.3",
        "symfony/form": "^3.4.24 || ^4.3",
        "symfony/framework-bundle": "^3.4.24 || ^4.3",
        "symfony/http-foundation": "^3.4.24 || ^4.3",
        "symfony/http-kernel": "^3.4.24 || ^4.3",
        "symfony/monolog-bundle": "^3.3",
        "symfony/options-resolver": "^3.4.24 || ^4.3",
        "symfony/proxy-manager-bridge": "^3.4.24 || ^4.3",
        "symfony/routing": "^3.4.24 || ^4.3",
        "symfony/security-core": "^3.4.24 || ^4.3",
        "symfony/security-bundle": "^3.4.24 || ^4.3",
        "symfony/translation": "^3.4.24 || ^4.3",
        "symfony/twig-bundle": "^3.4.24 || ^4.3",
        "symfony/validator": "^3.4.24 || ^4.3",
        "symfony/yaml": "^3.4.24 || ^4.3",
        "twig/twig": "^2.8.1",
        "sensio/framework-extra-bundle": "^5.3",
        "white-october/pagerfanta-bundle": "^1.2.4",
        "jean85/pretty-package-versions": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.1",
        "symfony/phpunit-bridge": "^4.3",
        "symfony/var-dumper": "^3.4.24 || ^4.3",
        "symfony/web-server-bundle": "^3.4.24 || ^4.3",
        "phpstan/phpstan": "^0.11",
        "phpstan/phpstan-strict-rules": "^0.11",
        "phpstan/phpstan-phpunit": "^0.11",
        "vimeo/psalm": "^3.4",
        "matthiasnoback/symfony-dependency-injection-test": "^4.0",
        "lchrusciel/api-test-case": "^4.1",
        "franzl/studio": "*"
    },
    "conflict": {
        "ocramius/proxy-manager": "<2.1"
    },
    "scripts": {
        "test": "@php vendor/bin/phpunit --colors=always",
        "test-api": "@php vendor/bin/phpunit -c phpunit-api.xml --colors=always",
        "coverage": "@php -dzend_extension=xdebug.so vendor/bin/phpunit --coverage-html=coverage --colors=always",
        "coverage-api": "@php -dzend_extension=xdebug.so vendor/bin/phpunit -c phpunit-api.xml --coverage-html=coverage_api --colors=always",
        "phpstan": "@php vendor/bin/phpstan analyse -c phpstan.neon --level=7 --ansi lib bundle",
        "phpstan-tests": "@php vendor/bin/phpstan analyse -c phpstan.tests.neon --level=7 --ansi tests",
        "psalm": "@php vendor/bin/psalm -c psalm.xml --show-info=false"
    },
    "autoload": {
        "psr-4": {
            "Netgen\\ContentBrowser\\": "lib/",
            "Netgen\\Bundle\\ContentBrowserBundle\\": "bundle/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Netgen\\ContentBrowser\\Tests\\": "tests/lib/",
            "Netgen\\ContentBrowser\\Tests\\App\\": "tests/application/src/",
            "Netgen\\Bundle\\ContentBrowserBundle\\Tests\\": "tests/bundle/"
        },
        "files": [
            "vendor/symfony/var-dumper/Resources/functions/dump.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
