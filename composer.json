{
    "name": "netgen/content-browser",
    "description": "Netgen Content Browser is a Symfony bundle that provides an interface which selects items from any kind of backend and returns the IDs of selected items back to the calling code.",
    "license": "proprietary",
    "authors": [
        {
            "name": "Netgen",
            "homepage": "https://www.netgenlabs.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-mbstring": "*",
        "netgen/content-browser-ui": "~0.12.0",
        "symfony/symfony": "^2.8.16 || ^3.4.4 || ^4.1.1",
        "twig/twig": "^1.35 || ^2.4.4",
        "sensio/framework-extra-bundle": "^3.0.12 || ^5.2",
        "white-october/pagerfanta-bundle": "^1.2",
        "jean85/pretty-package-versions": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7 || ^7.3",
        "symfony/phpunit-bridge": "^4.1",
        "phpstan/phpstan": "^0.10",
        "phpstan/phpstan-phpunit": "^0.10",
        "matthiasnoback/symfony-dependency-injection-test": "^1.2 || ^3.0",
        "symfony/monolog-bundle": "^3.3",
        "lakion/api-test-case": "^1.1 || ^3.0",
        "franzl/studio": "*"
    },
    "scripts": {
        "test": "@php vendor/bin/phpunit --colors=always",
        "test-api": "@php -dmemory_limit=-1 vendor/bin/phpunit -c phpunit-api.xml --colors=always",
        "coverage": "@php -dzend_extension=xdebug.so vendor/bin/phpunit --coverage-html=coverage --colors=always",
        "coverage-api": "@php -dzend_extension=xdebug.so vendor/bin/phpunit -c phpunit-api.xml --coverage-html=coverage_api --colors=always",
        "phpstan": "@php vendor/bin/phpstan analyse -c phpstan.neon --level=7 --ansi lib bundle",
        "phpstan-tests": "@php vendor/bin/phpstan analyse -c phpstan.tests.neon --level=7 --ansi tests"
    },
    "autoload": {
        "psr-4": {
            "Netgen\\ContentBrowser\\": "lib/",
            "Netgen\\Bundle\\ContentBrowserBundle\\": "bundle/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Netgen\\ContentBrowser\\Tests\\": "tests/lib/",
            "Netgen\\ContentBrowser\\Tests\\Kernel\\": "tests/application/app/",
            "Netgen\\Bundle\\ContentBrowserBundle\\Tests\\": "tests/bundle/"
        },
        "files": [
            "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "0.12.x-dev"
        }
    }
}
