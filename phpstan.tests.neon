includes:
    - vendor/phpstan/phpstan-phpunit/extension.neon
    - vendor/phpstan/phpstan-phpunit/rules.neon

parameters:
    excludes_analyse:
        - tests/application/web/bundles/
        - tests/bootstrap.php

    ignoreErrors:
        - '#assertStringContainsString\(\) expects string, array<string>\|string\|null given#'
        -
            message: '#Call to function method_exists#'
            path: tests/lib/TestCase/LegacyTestCaseTrait.php
