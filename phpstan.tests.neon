includes:
    - vendor/phpstan/phpstan-strict-rules/rules.neon
    - vendor/phpstan/phpstan-phpunit/extension.neon
    - vendor/phpstan/phpstan-phpunit/rules.neon

parameters:
    excludes_analyse:
        - tests/application/public/bundles/
        - tests/application/config/bootstrap.php
        - tests/bootstrap.php

    ignoreErrors:
        -
            message: '#assertStringContainsString\(\) expects string, array<string>\|string\|null given#'
            path: tests/bundle/Controller/API/RenderItemTest.php
